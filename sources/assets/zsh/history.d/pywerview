pywerview get-netcomputer -t "$TARGET" --user "$USERNAME" -k
pywerview get-localdisks --computername "$COMPUTER" --user "$USERNAME" -k
pywerview get-netuser --workgroup "$WORKGROUP" --dc-ip "$DC_IP" --cert cert.crt --key cert.key --username "$USERNAME" --attributes distinguishedname useraccountcontrol --tls
pywerview get-netgmsa -t "$TARGET" --user "$USERNAME" --hashes "$NT_HASH" --resolve-sids
pywerview get-netgmsa -t "$TARGET" --user "$USERNAME" --hashes "$NT_HASH" --resolve-sids --tls